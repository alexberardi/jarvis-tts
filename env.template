# LLM Proxy (for wake response generation)
JARVIS_LLM_PROXY_API_URL=http://localhost:8000
JARVIS_LLM_PROXY_API_VERSION=1

# Server port
TTS_PORT=8009

# Node authentication (required)
JARVIS_AUTH_BASE_URL=http://localhost:8007
JARVIS_APP_ID=jarvis-tts
JARVIS_APP_KEY=  # Get from jarvis-auth admin API

# Optional: Cache TTL for node auth validation (seconds)
NODE_AUTH_CACHE_TTL=60
